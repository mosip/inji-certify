{
	"vcTemplate": {
    "@context": [{{#each @context}}"{{context_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "issuer": "${_issuer}",
    "type": [{{#each type}}"{{type_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "issuanceDate": "${validFrom}",
    "expirationDate": "${validUntil}",
    "credentialSubject": {
      "id": "${_holderId}",
      "{{credential_key1}}": "{{credential_value1}}",
      "{{credential_key2}}": "{{credential_value2}}",
      "{{credential_key3}}": "{{credential_value3}}",
      "{{credential_key4}}": "{{credential_value4}}",
      "{{credential_key5}}": "{{credential_value5}}",
      "{{credential_key6}}": "{{credential_value6}}",
      "{{credential_key7}}": "{{credential_value7}}",
      "{{credential_key8}}": "{{credential_value8}}",
      "{{credential_key9}}": "{{credential_value9}}",
      "{{credential_key10}}": "{{credential_value10}}"                                               
  }},
    "contextURLs": [
	      {{#each contextURLs}}
	            "{{contextURLs}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
	"credentialTypes": [
	      {{#each credentialTypes}}
	            "{{credentialTypes}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],       	
    "credentialFormat": "{{credentialFormat}}",
    "credentialConfigKeyId": "{{credentialConfigKeyId}}",    
    "didUrl": "{{didUrl}}",        
    "keyManagerAppId" : "{{keyManagerAppId}}",
    "keyManagerRefId" : "{{keyManagerRefId}}",
    "signatureCryptoSuite" : "{{signatureCryptoSuite}}",    
    "displayOrder" : [
	      {{#each displayOrder}}
	            "{{displayOrder_values}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "scope" : "{{scope}}",
    "metaDataDisplay" : [{
    "name": "{{display_name}}",
    "locale": "en",
    "logo": {
      "url": "https://mosip.github.io/inji-config/logos/mosipid-logo.png",
      "alt_text": "a square logo of a MOSIP"
    },
    "background_color": "#12107c",
    "text_color": "#FFFFFF"
  }],
    "credentialSubjectDefinition" : {
    "fullName": {
        "display": [
            {
                "name": "Full Name", 
                "locale": "en"
            }
        ]
    },
    "phone": {
        "display": [
            {
                "name": "Phone Number", 
                "locale": "en"
            }
        ]
    },
    "dateOfBirth": {
        "display": [
            {
                "name": "Date of Birth", 
                "locale": "en"
            }
        ]
    },
    "gender": {
        "display": [
            {
                "name": "Gender", 
                "locale": "en"
            }
        ]
    },
    "email": {
        "display": [
            {
                "name": "Email Id", 
                "locale": "en"
            }
        ]
    },
    "region": {
        "display": [
            {
                "name": "Region", 
                "locale": "en"
            }
        ]
    },
    "addressLine1": {
        "display": [
            {
                "name": "Address Line 1", 
                "locale": "en"
            }
        ]
    },
    "UIN": {
        "display": [
            {
                "name": "UIN", 
                "locale": "en"
            }
        ]
