{
	"vcTemplate": {
    "@context": [{{#each @context}}"{{context_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "issuer": "${_issuer}",
    "type": [{{#each type}}"{{type_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "validFrom": "${validFrom}",
    "validUntil": "${validUntil}",
    "credentialSubject": {
      "id": "${_holderId}",
      "{{credential_key1}}": "{{credential_value1}}",
      "{{credential_key2}}": "{{credential_value2}}",
      "{{credential_key3}}": "{{credential_value3}}",
      "{{credential_key4}}": "{{credential_value4}}",
      "{{credential_key5}}": "{{credential_value5}}",
      "{{credential_key6}}": "{{credential_value6}}",
      "{{credential_key7}}": "{{credential_value7}}",
      "{{credential_key8}}": "{{credential_value8}}",
      "{{credential_key9}}": "{{credential_value9}}",
      "{{credential_key10}}": "{{credential_value10}}",
      "{{credential_key11}}": "{{credential_value11}}",
      "{{credential_key12}}": "{{credential_value12}}",
      "{{credential_key13}}": "{{credential_value13}}",
      "{{credential_key14}}": "{{credential_value14}}",
      "{{credential_key15}}": "{{credential_value15}}",
      "{{credential_key16}}": "{{credential_value16}}",
      "{{credential_key17}}": "{{credential_value17}}",
      "{{credential_key18}}": "{{credential_value18}}",
      "{{credential_key19}}": "{{credential_value19}}"                                               
  }},
    "contextURLs": [
	      {{#each contextURLs}}
	            "{{contextURLs}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
	"credentialTypes": [
	      {{#each credentialTypes}}
	            "{{credentialTypes}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],       	
    "credentialFormat": "{{credentialFormat}}",
    "credentialConfigKeyId": "{{credentialConfigKeyId}}",
    "didUrl": "{{didUrl}}",        
    "keyManagerAppId" : "{{keyManagerAppId}}",
    "keyManagerRefId" : "{{keyManagerRefId}}",
    "signatureAlgo" : "{{signatureAlgo}}",
    "signatureCryptoSuite" : "{{signatureCryptoSuite}}",    
    "displayOrder" : [
	      {{#each displayOrder}}
	            "{{displayOrder_values}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "scope" : "{{scope}}",
    "metaDataDisplay" : [{
    "name": "{{display_name}}",
    "locale": "en",
    "logo": {
      "url": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png",
      "alt_text": "land statement Logo"
    },
    "background_color": "#ebfaff",
    "background_image": {"uri": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png"},    
    "text_color": "#000000"
  }],
    "credentialStatusPurposes": [
	      {{#each credentialStatusPurposes}}
	            "{{credentialStatusPurposes}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],  
    "credentialSubjectDefinition" : {
    "NumberOfCAR": {
        "display": [
            {
                "name": "CAR Registration Number", 
                "locale": "en"
            }
        ]
    },
    "RegistrationDate": {
        "display": [
            {
                "name": "Registration Date", 
                "locale": "en"
            }
        ]
    }
  }
}