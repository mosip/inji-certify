{
	"vcTemplate": {
    "@context": [{{#each @context}}"{{context_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "issuer": "${_issuer}",
    "type": [{{#each type}}"{{type_value}}"{{#unless @last}}, {{/unless}}{{/each}}],
    "validFrom": "${validFrom}",
    "validUntil": "${validUntil}",
    "credentialSubject": {
      "id": "${_holderId}",
      "{{credential_key1}}": "{{credential_value1}}",
      "{{credential_key2}}": "{{credential_value2}}",
      "{{credential_key3}}": "{{credential_value3}}",
      "{{credential_key4}}": "{{credential_value4}}",
      "{{credential_key5}}": "{{credential_value5}}",
      "{{credential_key6}}": "{{credential_value6}}",
      "{{credential_key7}}": "{{credential_value7}}",
      "{{credential_key8}}": "{{credential_value8}}",
      "{{credential_key9}}": "{{credential_value9}}"                                               
  }},
	"credentialType": [
	      {{#each credentialType}}
	            "{{credentialType}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],       	
    "credentialFormat": "{{credentialFormat}}",
    "credentialConfigKeyId": "{{credentialConfigKeyId}}",    
    "didUrl": "{{didUrl}}",        
    "keyManagerAppId" : "{{keyManagerAppId}}",
    "keyManagerRefId" : "{{keyManagerRefId}}",
    "signatureAlgo" : "{{signatureAlgo}}",
    "sdClaim" : "{{sdClaim}}",
    "order" : [
	      {{#each order}}
	            "{{order_values}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "scope" : "{{scope}}",
    "display" : [{
    "name": "{{display_name}}",
    "locale": "en",
    "logo": {
      "url": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png",
      "alt_text": "land statement Logo"
    },
    "background_color": "#ebfaff",
    "text_color": "#000000"
  }],
    "cryptographic_binding_methods_supported" : [
	      {{#each cryptographic_binding_methods_supported}}
	            "{{cryptographic_binding_methods_supported_value}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "credential_signing_alg_values_supported" : [
	      {{#each credential_signing_alg_values_supported}}
	            "{{credential_signing_alg_values_supported_value}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "proof_types_supported": {
    "jwt": {
      "proof_signing_alg_values_supported": [
        {{#each proof_signing_alg_values_supported}}
          "{{algs}}"
          {{#unless @last}},{{/unless}}
        {{/each}}
      ]
    }
  },
    "credentialSubject" : {
    "PermanentPreservationAreas": {
        "display": [
            {
                "name": "Permanent Preservation Areas", 
                "locale": "en"
            }
        ]
    },
    "APPInConsolidatedRuralArea": {
        "display": [
            {
                "name": "APP in Consolidated Rural Area", 
                "locale": "en"
            }
        ]
    }
  }
}