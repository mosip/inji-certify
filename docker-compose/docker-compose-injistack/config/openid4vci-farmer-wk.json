                    "display": [{"name": "Farmer Profile Verifiable Credential",
                        "locale": "en", 
                        "logo": {"url": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png","alt_text": "a square logo of Agricultural department"},
                        "background_color": "#FDFAF9",
                        "background_image": { "uri": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png" },
                        "text_color": "#7C4616"}],
