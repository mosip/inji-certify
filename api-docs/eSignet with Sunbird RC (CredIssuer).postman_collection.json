			"name": "Sunbird Registry",
			"item": [
				{
					"name": "Generate DID (Issuer)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// JSON data",
									"var jsonData = pm.response.json();",
									"",
									"// Extracting the ID from the response",
									"var id = jsonData[0].id;",
									"",
									"// Store the id in an environment variable",
									"pm.environment.set(\"author_did_id\", id);",
									"",
									"// Creating the folder name",
									"var folderName = id.substring(id.lastIndexOf(\":\") + 1);",
									"",
									"// Content of the file",
									"var content = JSON.stringify(jsonData[0]);",
									"",
									"// Convert JSON to base64",
									"var contentBase64 = btoa(content);",
									"",
									"console.log(folderName)",
									"",
									"// GitHub API endpoint",
									"var githubApiUrl = \"https://api.github.com/repos/chandru-engineer/identity/contents/\" + folderName + \"/did.json\";",
									"",
									"// Retrieve existing file details",
									"pm.sendRequest({",
									"    url: githubApiUrl,",
									"    method: 'GET',",
									"    header: {",
									"        'Authorization': 'Bearer ' + pm.globals.get(\"github_token\"),",
									"        'Content-Type': 'application/json'",
									"    }",
									"}, function (err, response) {",
									"    if (err) {",
									"        console.error(err);",
									"        return;",
									"    }",
									"    ",
									"    var sha = response.json().sha;",
									"    ",
									"    // Create the request payload with sha",
									"    var requestPayload = {",
									"        \"message\": \"Update DID file via API\",",
									"        \"content\": contentBase64,",
									"        \"sha\": sha",
									"    };",
									"    ",
									"    // Send request to update the file",
									"    pm.sendRequest({",
									"        url: githubApiUrl,",
									"        method: 'PUT',",
									"        header: {",
									"            'Authorization': 'Bearer ' + pm.globals.get(\"github_token\"),",
									"            'Content-Type': 'application/json'",
									"        },",
									"        body: JSON.stringify(requestPayload)",
									"    }, function (err, response) {",
									"        if (err) {",
									"            console.error(err);",
									"            return;",
									"        }",
									"        ",
									"        console.log(response.json());",
									"    });",
									"});",
									"",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"content\": [\n        {\n            \"alsoKnownAs\": [\n                \"issuer@ooru.io\"\n            ],\n            \"method\": \"web\",\n            \"description\": \"This is an example DID for a fictional entity.\",\n            \"logo\": \"https://example.com/logo.png\",\n            \"contactPoint\": [\n                {\n                    \"name\": \"Example Support\",\n                    \"email\": \"support@example.com\",\n                    \"telephone\": \"+1-800-555-1234\"\n                }\n            ]\n        }\n    ]\n}"
						},
						"url": {
							"raw": "{{sunbird_url}}/identity/did/generate",
							"host": [
								"{{sunbird_url}}"
							],
							"path": [
								"identity",
								"did",
								"generate"
							]
						}
					},
					"response": []
				},
				{
					"name": "Create Schema",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if the status code is 201",
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"// Parse the response JSON",
									"let responseData = pm.response.json();",
									"",
									"// Extract the id from the schema object in the response",
									"let schemaId = responseData.schema.id;",
									"",
									"// Store the id in an environment variable",
									"pm.environment.set(\"schema_id\", schemaId);",
									"",
									"// Log the id to the console (optional, for debugging)",
									"console.log(\"Schema ID:\", schemaId);",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"schema\": {\n        \"type\": \"https://w3c-ccg.github.io/vc-json-schemas/\",\n        \"version\": \"1.0.0\",\n        \"id\": \"did:ulpschema:c9cc0f03-4f94-4f44-9bcd-b24a8696fa2\",\n        \"name\": \"Proof of Medical Insurance Credential\",\n        \"author\": \"{{author_did_id}}\",\n        \"authored\": \"2022-12-19T09:22:23.064Z\",\n        \"schema\": {\n            \"$id\": \"Proof-of-Medical-Insurance-Credential-1.0\",\n            \"$schema\": \"https://json-schema.org/draft/2019-09/schema\",\n            \"description\": \"The holder is holding the certificate of Insurance\",\n            \"type\": \"object\",\n            \"properties\": {\n                \"policyNumber\": {\n                    \"type\": \"string\"\n                },\n                \"policyName\": {\n                    \"type\": \"string\"\n                },\n                \"policyExpiresOn\": {\n                    \"type\": \"string\"\n                },\n                \"policyIssuedOn\": {\n                    \"type\": \"string\"\n                },\n                \"benefits\": {\n                    \"type\": \"array\",\n                    \"items\": {\n                        \"type\": \"string\"\n                    }\n                },\n                \"fullName\": {\n                    \"type\": \"string\",\n                    \"title\": \"Full Name\"\n                },\n                \"dob\": {\n                    \"type\": \"string\"\n                },\n                \"gender\": {\n                    \"type\": \"string\",\n                    \"enum\": [\n                        \"Male\",\n                        \"Female\",\n                        \"Other\"\n                    ]\n                }\n            },\n            \"required\": [\n                \"policyNumber\",\n                \"policyName\",\n                \"fullName\",\n                \"dob\"\n            ],\n            \"additionalProperties\": true\n        }\n    },\n    \"tags\": [\n        \"InsuranceCertificate\"\n    ],\n    \"status\": \"DRAFT\"\n}\n"
						},
						"url": {
							"raw": "{{sunbird_url}}/schema/credential-schema",
							"host": [
								"{{sunbird_url}}"
							],
							"path": [
								"schema",
								"credential-schema"
							]
						}
					},
					"response": []
				},
				{
					"name": "New Request",
					"request": {
						"method": "PUT",
						"header": [],
						"url": {
							"raw": "{{sunbird_url}}/schema/credential-schema/publish/{{schema_id}}/1.0.0",
							"host": [
								"{{sunbird_url}}"
							],
							"path": [
								"schema",
								"credential-schema",
								"publish",
								"{{schema_id}}",
								"1.0.0"
							]
						}
					},
					"response": []
				}
			],
			"description": "One time Creation.\n\n1. Create the DID id and configure in esignet Properties file.\n2. Create schema and publish it and add in esignet properties file.\n    \n\nNote. We need to store the `DID` id in some where online to make it web resolvable.\n\nIn my case, i am storeing it in `chandru-engineer.github.io/identity.`"
		},
